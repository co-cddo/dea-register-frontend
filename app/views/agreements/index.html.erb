<h1 class="govuk-heading-l">Agreements</h1>

<% if @agreements.present? %>
  <ul class="govuk-list">
  <% @agreements.each do |agreement| %>
    <li>
      <%= link_to agreement.name, agreement, class: "govuk-link" %>
    </li>
  <% end %>
  </ul>

  <%= govuk_pagination(pagy: @pagy) %>
<% else %>
  <p>No matching records found<p>
<% end %>
